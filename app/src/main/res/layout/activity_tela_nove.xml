<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TelaNove">

    <com.google.android.material.appbar.MaterialToolbar
            android:layout_width="528dp"
            android:layout_height="76dp"
            android:background="@color/black"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/materialToolbar"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="1.0"/>
    <Button
            android:text="Inicio"
            android:onClick="inicio"
            android:layout_width="79dp"
            android:layout_height="61dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/bt6"
            app:layout_constraintHorizontal_bias="0.003" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.006"
            android:textColor="#FAFAFA" android:fontFamily="serif" android:backgroundTint="@color/black"
            android:textSize="10sp" android:textAlignment="center"/>
    <Button
            android:text="Sobre"
            android:onClick="sobre"
            android:layout_width="91dp"
            android:layout_height="65dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/bt7"
            app:layout_constraintHorizontal_bias="0.25" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.004"
            android:textColor="#FAFAFA" android:fontFamily="serif" android:backgroundTint="@color/black"
            android:textSize="10sp" android:textAlignment="center"/>
    <Button
            android:text="Contato"
            android:onClick="contato"
            android:layout_width="105dp"
            android:layout_height="58dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/bt8"
            app:layout_constraintHorizontal_bias="0.558" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.009"
            android:textColor="#FAFAFA" android:fontFamily="serif" android:backgroundTint="@color/black"
            android:textSize="10sp"/>
    <Button
            android:text="Planos"
            android:onClick="plano"
            android:layout_width="86dp"
            android:layout_height="58dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/bt9"
            app:layout_constraintHorizontal_bias="0.851" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.008"
            android:textColor="#FAFAFA" android:fontFamily="serif" android:backgroundTint="@color/black"
            android:textSize="10sp"/>
    <ImageView
            android:layout_width="491dp"
            android:layout_height="156dp" app:srcCompat="@drawable/logo"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView2" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.111" app:layout_constraintHorizontal_bias="0.0"/>
    <com.google.android.material.appbar.MaterialToolbar
            android:layout_width="393dp"
            android:layout_height="191dp"
            android:background="#D3D3D3"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/materialToolbar1" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.47"/>
    <TextView
            android:text="Grato pelo agendamento! Valorizamos sua preferência e estamos ansiosos para atendê-lo(a) no horário escolhido. Até breve!"
            android:layout_width="346dp"
            android:layout_height="147dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView3"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:textAlignment="center" android:fontFamily="serif" android:textSize="23sp"
            app:layout_constraintHorizontal_bias="0.492" app:layout_constraintVertical_bias="0.464"/>



</androidx.constraintlayout.widget.ConstraintLayout>